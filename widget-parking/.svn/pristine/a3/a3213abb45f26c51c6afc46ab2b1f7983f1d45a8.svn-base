/*notification cover start*/
.notification_clear
{
	clear: both;
}
.notification_cover
{
	width: 616px;
	height: 335px;
	background-color: #000;
	background-repeat: repeat;	
	position: absolute;
	z-index: 2;
	display: none;	
	text-align: center;	
}
.notification_center_content
{		
	width: 616px; 
	height: 335px;
	background-image: url("./images/widget_notification_bg.png");
	background-repeat: no-repeat;
	text-align:center;
}
.notification_text
{
	height: 260px;			
	width:100%;
	text-align:center;	
	font: normal normal 30px "Gill Sans PSA", arial, helvetica, verdana, sans-serif;
	color:#ffffff;
	padding-left:15px;
	padding-right:15px;
	display:table;
}
.notification_text_content
{
	width:100%;
	height:100%;
	text-align:center;
	vertical-align:middle;
	display:table-cell;
}
.notification_buttons
{
	margin-top: 2px;
	float:right;
	font: normal normal 26px "Gill Sans PSA", arial, helvetica, verdana, sans-serif;
	color:#fff;
}
.notification_ok_text
{
	margin-left: 15px;
	margin-top: 30px;
}
.notification_ok_button_off
{
	background:url('./images/widget_notification_ok_button.png') no-repeat bottom;	
	width:172px;
	height:73px;	
	text-align:center;
	float: right;
}
.notification_ok_button_on
{
	background:url('./images/widget_notification_ok_button.png') no-repeat top;	
	width:172px;
	height:73px;		
	text-align:center;
	float: right;
}
/*confirmation cover end*/